{
  "builds": [
    { "src": "vercel-frontend/**/*", "use": "@vercel/static" },
    { "src": "static/img/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/", "dest": "/vercel-frontend/index.html" },
    { "src": "/(.*)", "dest": "/vercel-frontend/$1" }
  ]
}
